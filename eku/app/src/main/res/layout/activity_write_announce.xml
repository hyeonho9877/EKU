<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="296dp"
        android:fontFamily="@font/font_noto_sans_kr"
        android:includeFontPadding="false"
        android:text="Write"
        android:textColor="@color/kyonggi_color"
        android:textSize="35sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.652"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/write_announce_title"
        android:layout_width="280dp"
        android:layout_height="53dp"
        android:layout_marginStart="20dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="공지 게시글 제목을 입력하세요"
        android:inputType="textMultiLine"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/textView14" />

    <TextView
        android:id="@+id/write_announce_building"
        android:layout_width="74dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/font_noto_sans_kr"
        android:includeFontPadding="false"
        android:text="강의동"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <EditText
        android:id="@+id/write_announce_content"
        android:layout_width="410dp"
        android:layout_height="283dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="1dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="공지 게시글 내용을 입력하세요"
        android:inputType="textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/building8" />

    <Button
        android:id="@+id/write_announce_save"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="208dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_blue"
        android:text="submit"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/write_announce_close"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/rounded_blue"
        android:text="cancel"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <CheckBox
        android:id="@+id/building0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="제2공"
        app:layout_constraintStart_toEndOf="@+id/building5"
        app:layout_constraintTop_toTopOf="@+id/building5" />

    <CheckBox
        android:id="@+id/building5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="종강"
        app:layout_constraintStart_toEndOf="@+id/building8"
        app:layout_constraintTop_toTopOf="@+id/building8" />

    <CheckBox
        android:id="@+id/building6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="6강"
        app:layout_constraintStart_toEndOf="@+id/write_announce_building"
        app:layout_constraintTop_toTopOf="@+id/write_announce_building" />

    <CheckBox
        android:id="@+id/building7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="7강"
        app:layout_constraintStart_toEndOf="@+id/building6"
        app:layout_constraintTop_toTopOf="@+id/building6" />

    <CheckBox
        android:id="@+id/building8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="8강"
        app:layout_constraintStart_toEndOf="@+id/building7"
        app:layout_constraintTop_toTopOf="@+id/building7" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/font_noto_sans_kr"
        android:includeFontPadding="false"
        android:text="제목"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

</androidx.constraintlayout.widget.ConstraintLayout>